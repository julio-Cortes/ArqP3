DATA:
    inicio 
    fin
    contador 0
    cantidad 0
CODE:
siguiente:
    MOV B,(inicio)
    MOV A,(B)
    AND A,1
    MOV B,(contador)
    ADD (contador) 
    INC (cantidad)
direccionamiento:
    INC (inicio)
    MOV B,(final)
    CMP B,(inicio)
    JGE siguiente
calculo:
    MOV A,(cantidad)
    SUB A,(contador)